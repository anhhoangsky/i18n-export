{
  "name": "i18n-export",
  "version": "1.0.10",
  "description": "Convert between JSON and Excel files",
  "main": "to-excel.js",
  "bin": {
    "i18n-export": "./src/to-excel.js",
    "i18n-import": "./src/to-json.js"
  },
  "scripts": {
    "convert:to-excel": "node ./src/to-excel.js",
    "convert:to-json": "node ./src/to-json.js",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest"
  },
  "author": "AnhSky",
  "license": "ISC",
  "dependencies": {
    "commander": "^12.1.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "chai": "^5.1.1",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0"
  },
  "type": "module",
  "keywords": [
    "i18n",
    "export",
    "json",
    "excel"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anhhoangsky/i18n-export"
  },
  "bugs": {
    "url": "https://github.com/anhhoangsky/i18n-exportissues"
  },
  "homepage": "https://github.com/anhhoangsky/i18n-export#readme"
